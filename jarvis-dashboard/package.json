{
  "name": "jarvis-dashboard",
  "private": true,
  "version": "0.0.0",
  "description": "Jarvis AI Dashboard",
  "author": "Yash",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none npm run start:web\" \"wait-on http://localhost:5173 && npm run start:electron\"",
    "start:web": "vite",
    "start:electron": "cross-env ELECTRON_START_URL=http://localhost:5173 electron .",
    "build": "vite build && electron-builder --dir",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "autoprefixer": "^10.4.20",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-webcam": "^7.2.0",
    "tailwindcss": "^3.4.1",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.1.4",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.jarvis.dashboard",
    "productName": "Jarvis Dashboard",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}